-- Function to show a notification using OrionLib
local function showNotification(title, content)
    OrionLib:MakeNotification({
        Name = title,
        Content = content,
        Image = "rbxassetid://7733964719", -- Your notification image
        Time = 5
    })
end

-- Create a new sound instance
local openingSound = Instance.new("Sound")
openingSound.SoundId = "rbxassetid://6958727243"  -- Replace with your actual sound asset ID
openingSound.Volume = 5  -- You can adjust the volume if needed
openingSound.PlayOnRemove = false
openingSound.Looped = false
openingSound.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui") -- Parent to Player's GUI

-- Function to play the opening sound
local function playOpeningSound()
    openingSound:Play()
end

-- Assuming this is the part of your script where the GUI opens
playOpeningSound()

-- Rest of your GUI code


