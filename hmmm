local Tab = Window:MakeTab({
    Name = "Custom",
    Icon = "rbxassetid://7743871002",
    PremiumOnly = false
})
local Section = Tab:AddSection({
    Name = "Customize your walking/idle/run and etc."
})

local Section = Tab:AddSection({
    Name = "Idle"
})

