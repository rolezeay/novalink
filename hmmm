Players.PlayerAdded:Connect(function(player)
    showNotification("Oyuncu Katıldı", player.Name .. " has joined the game.")
end)

-- Player left event
Players.PlayerRemoving:Connect(function(player)
    showNotification("Oyuncu Çıktı", player.Name .. " has left the game.")
end)


